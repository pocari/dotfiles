# vim: ft=sh 

# # profileするときは下のコメントを外す
# zmodload zsh/zprof && zprof
# 
# if (which zprof > /dev/null 2>&1) ; then
#   zprof
# fi
# # profile用ここまで

# rbenv
if [ -d ${HOME}/.rbenv ] ; then
  PATH=${HOME}/.rbenv/bin:/usr/local/bin:${PATH}
  export PATH
  eval "$(rbenv init -)"
fi

# nodenv
if [ -d ${HOME}/.nodenv ] ; then
  PATH=${HOME}/.nodenv/bin:$PATH
  eval "$(nodenv init -)"
fi

